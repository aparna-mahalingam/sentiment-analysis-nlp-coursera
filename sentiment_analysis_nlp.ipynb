   "outputs": [],
   "source": [
    "# natural language processing toolkit nltk\n",
    "from nltk.stem.porter import PorterStemmer\n",
    "\n",
    "porter = PorterStemmer()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "# tokenizer -- splits sentence by spaces into words\n",
    "def tokenizer(text):\n",
    "    return(text.split())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "# stemming removes the suffixes of word forms or derived words of common words\n",
    "def stemming_tokenizer(text):\n",
    "    # reduntancy, not considering tokenizer()\n",
    "    return [porter.stem(word) for word in text.split()]"
   ]
  },
